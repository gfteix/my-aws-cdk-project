{
  "name": "lambda-gateway-cdk",
  "license": "MIT",
  "version": "0.1.0",
  "bin": {
    "lambda-gateway-cdk": "bin/index.ts"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "synth": "CURRENT_ENV=DEV cdk synth MainStack-DEV --no-staging > template.yaml",
    "deploy": "npx cdk deploy --context CURRENT_ENV=$CURRENT_ENV --all --require-approval never --outputs-file output.json",
    "destroy": "npx cdk destroy --context CURRENT_ENV=$CURRENT_ENV  --all --force"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/jest": "^29.5.8",
    "@types/node": "20.9.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "aws-cdk": "2.114.1",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.11",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "http-status-codes": "^2.3.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "*"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.470.0",
    "@aws-sdk/lib-dynamodb": "^3.470.0",
    "aws-cdk-lib": "2.114.1",
    "constructs": "^10.0.0",
    "eslint": "^8.0.1",
    "source-map-support": "^0.5.21",
    "zod": "^3.22.4"
  }
}
